<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.model.CertiVO">
	<insert id="write" parameterType="CertiVO">
		insert into certiInfo values (cer_num_seq.nextval, #{id}, #{storeName}, #{review}, #{fileName})
	</insert>
	
	<select id="BoardList" parameterType="CertiVO" resultType="CertiVO">
	select * from certiInfo
	</select>
	
</mapper>